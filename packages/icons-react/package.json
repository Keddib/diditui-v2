{
  "name": "@diditui/icons-react",
  "version": "0.0.0",
  "license": "MIT",
  "main": "./dist/cjs/diditui-icons-react.cjs",
  "types": "./dist/esm/diditui-icons-react.d.ts",
  "module": "./dist/esm/diditui-icons-react.mjs",
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "pnpm run clean && pnpm run build:icons && pnpm typecheck && pnpm run build:bundles",
    "build:icons": "node build.mjs",
    "build:bundles": "rollup -c ./rollup.config.mjs",
    "clean": "rm -rf dist && find . ! -name '.gitkeep' -path '*/src/icons/*' -exec rm -rf {} +",
    "typecheck": "tsc",
    "imports-check": "attw $(npm pack)"
  },
  "dependencies": {
    "@diditui/icons": "workspace:*"
  },
  "devDependencies": {
    "@diditui/eslint-config": "workspace:*",
    "@diditui/typescript-config": "workspace:*",
    "@types/react": "18.2.60",
    "eslint": "^8.57.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "rollup": "^4.44.0",
    "rollup-plugin-dts": "^6.2.1",
    "typescript": "5.5.4"
  },
  "peerDependencies": {
    "react": ">= 16"
  }
}
